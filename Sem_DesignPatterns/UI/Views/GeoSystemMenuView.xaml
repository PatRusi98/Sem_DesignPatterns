<UserControl x:Class="Sem_DesignPatterns.GeoSystemMenuView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Menu>
        <MenuItem Header="Parcel">
            <MenuItem Header="Insert" Click="OnInsertParcelClick"/>
            <MenuItem Header="Search" Click="OnSearchParcelClick"/>
            <MenuItem Header="Find all" Click="OnFindAllParcelsClick"/>
        </MenuItem>
        <MenuItem Header="Property">
            <MenuItem Header="Insert" Click="OnInsertPropertyClick"/>
            <MenuItem Header="Search" Click="OnSearchPropertyClick"/>
            <MenuItem Header="Find all" Click="OnFindAllPropertiesClick"/>
        </MenuItem>
        <MenuItem Header="All Objects">
            <MenuItem Header="Find by point" Click="OnFindByPointClick"/>
            <MenuItem Header="Find all objects" Click="OnFindAllObjectsClick"/>
        </MenuItem>
        <MenuItem Header="Generator">
            <MenuItem Header="Generate...">
                <MenuItem Header="Parcels" Click="OnGenerateParcelsClick"/>
                <MenuItem Header="Properties" Click="OnGeneratePropertiesClick"/>
                <MenuItem Header="Random objects" Click="OnGenerateRandomObjectsClick"/>
            </MenuItem>
            <MenuItem Header="Random operations" Click="OnRandomOperationsClick"/>
        </MenuItem>
    </Menu>
</UserControl>
